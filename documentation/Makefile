documentation = paper.tex

default:
	@echo "Use paper|clean"
	@echo "Use"
	@echo "   make paper\tto build the paper.pdf"
	@echo "   make clean\tto remove tmp files generated by pdflatex"

all: paper

paper:
	pdflatex -interaction=nonstopmode $(documentation)
	pdflatex -interaction=nonstopmode $(documentation)


clean:
	rm -f *.aux *.log *.aux *.out *.nav *.snm *.toc




