<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0"
	spacing="40.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.AccessControlEndController" styleClass="userTerminal">
	<children>
		<Label fx:id="accessNote" alignment="TOP_CENTER" text="Bitte Karte einlegen">
			<font>
				<Font name="Consolas Bold" size="22.0" />
			</font>
		</Label>
		<Label fx:id="accessLamp" alignment="TOP_CENTER" blendMode="HARD_LIGHT"
			minWidth="100" prefHeight="27.0" prefWidth="527.0"
			style="-fx-background-color: #FF0000; -fx-border-radius: 90 90 90 90; -fx-background-radius: 90 90 90 90;"
			VBox.vgrow="SOMETIMES">
			<padding>
				<Insets top="10.0" />
			</padding>
			<opaqueInsets>
				<Insets />
			</opaqueInsets>
			<VBox.margin>
				<Insets bottom="40.0" />
			</VBox.margin>
		</Label>
		<Button fx:id="insertCard" alignment="CENTER" defaultButton="true"
			mnemonicParsing="false" onAction="#handleEndAction" text="Karte einlegen"
			textAlignment="CENTER">
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
			<font>
				<Font name="System Bold" size="13.0" />
			</font>
		</Button>
	</children>
	<padding>
		<Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
	</padding>
</VBox>
